<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/hShopper_background"
    tools:context=".MyActivity">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:text="Partneriai"
        android:id="@+id/labelPartners"
        android:textSize="25dp"
        android:textIsSelectable="false"
        android:textColor="#ffffffff"
        android:layout_alignParentTop="true"
        android:background="@color/hShopper_extra"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false"
        android:gravity="center_horizontal"
        android:paddingTop="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userRights"
        android:layout_below="@+id/labelPartners"
        android:layout_alignParentStart="true"
        android:src="@drawable/audimas"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/returnHomeButton"
        android:layout_below="@+id/labelPartners"
        android:layout_toRightOf="@+id/userRights"
        android:src="@drawable/baltameska"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/selectCategory"
        android:layout_below="@+id/labelPartners"
        android:layout_toRightOf="@+id/returnHomeButton"
        android:src="@drawable/bmsmegapolis"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_below="@+id/userRights"
        android:layout_alignParentStart="true"
        android:src="@drawable/celio"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:layout_below="@+id/returnHomeButton"
        android:layout_toRightOf="@+id/imageView4"
        android:src="@drawable/danija"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:layout_below="@+id/imageView4"
        android:layout_alignParentStart="true"
        android:src="@drawable/deichmann"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView7"
        android:layout_alignTop="@+id/imageView6"
        android:layout_toRightOf="@+id/imageView6"
        android:src="@drawable/ecco"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView8"
        android:layout_alignTop="@+id/imageView7"
        android:layout_toRightOf="@+id/imageView7"
        android:src="@drawable/hm"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView9"
        android:layout_below="@+id/imageView6"
        android:layout_alignParentStart="true"
        android:src="@drawable/sportland"
        android:layout_margin="5dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView10"
        android:layout_below="@+id/imageView7"
        android:layout_toRightOf="@+id/imageView9"
        android:src="@drawable/topocentras"
        android:layout_margin="5dp" />

</RelativeLayout>
